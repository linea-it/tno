  "dependencies": {
    "axios": "^0.18.0",
    "chartist": "^0.10.1",
    "classnames": "^2.2.5",
    "create-react-app": "^1.3.3",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "primeicons": "^1.0.0-beta.10",
    "primereact": "^2.0.0-beta.1",
    "prop-types": "^15.6.1",
    "rc-slider": "^8.6.1",
    "react": "^16.0.0",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-table-next": "^0.1.8",
    "react-bootstrap-table2-overlay": "^0.1.1",
    "react-bootstrap-table2-paginator": "^0.1.1",
    "react-chartist": "^0.13.0",
    "react-dom": "^16.0.0",
    "react-images": "^0.5.19",
    "react-interval": "^2.0.2",
    "react-lightbox-component": "^1.2.1",
    "react-notification-system": "^0.2.15",
    "react-numeric-input": "^2.2.3",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.10",
    "react-select": "^1.2.1",
    "react-syntax-highlight": "^15.3.1",
    "serve": "^8.1.2",
    "sql-formatter": "^2.3.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "serve": "serve -s build -l 7000"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.11.1"
  }
}
